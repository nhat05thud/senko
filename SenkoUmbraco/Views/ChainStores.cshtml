@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Master.cshtml";
    var current = CurrentPage;
}

@* the fun starts here *@
@Html.Partial("/Views/Partials/Layout/_Banner.cshtml")
<div class="container">
    <div class="wrap-920">
        <h1 class="page-title">@current.Name</h1>
        <div class="wrap-systems">
            @foreach (var item in current.Children())
            {
                <div class="item">
                    <div class="contact-box">
					<iframe src="@Html.Raw(item.GetPropertyValue("Map"))" width="100%" height="260" frameborder="0" style="border: 0px;"></iframe>
                        
                    </div>
                </div>
            }
        </div>
    </div>
</div>